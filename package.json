{
  "name": "sample-webpack-example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:dockerimage": "docker build -t mamba_docker_image .",
    "build:empack": "docker run  -it --mount type=bind,source=${PWD}/src,target=/wasm   mamba_docker_image cp -a output/. /wasm",
    "build:copy_pyjs": "docker run  -it --mount type=bind,source=${PWD}/src,target=/wasm   mamba_docker_image cp -a /opt/conda/envs/pyjs-wasm-env/lib_js/pyjs/. /wasm",
    "get_wasm": "npm run build:dockerimage && npm run build:empack && npm run build:copy_pyjs && node add_export_to_js",
    "copy_to_source": "npm run build:dockerimage && npm run build:empack && npm run build:copy_pyjs",
    "bash": "docker run  -it --mount type=bind,source=${PWD}/wasm,target=/wasm   mamba_docker_image bash",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack serve --mode development",
    "watch": "webpack --watch",
    "build": "npm run get_wasm && webpack --mode production"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "copy-webpack-plugin": "^9.0.1",
    "copyfiles": "^2.4.1",
    "exports-loader": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.0"
  },
  "dependencies": {
    "@egjs/flicking": "^4.9.2",
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^3.4.1",
    "jquery": "^3.6.0",
    "panzoom": "^9.4.3",
    "popper.js": "^1.16.1"
  }
}
